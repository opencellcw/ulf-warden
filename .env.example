# Google Cloud Platform
PROJECT_ID=seu-projeto-gcp
REGION=us-central1
ZONE=us-central1-a
CLUSTER_NAME=ulf-cluster

# Anthropic (obrigatório)
ANTHROPIC_API_KEY=sk-ant-api03-xxx

# Cloudflare AI Gateway (opcional - routing, caching, analytics)
# https://developers.cloudflare.com/ai-gateway/
CLOUDFLARE_ACCOUNT_ID=seu-cloudflare-account-id
CLOUDFLARE_GATEWAY_SLUG=ulf-gateway  # Nome do seu gateway no Cloudflare

# Security Configuration (OpenClaw-Security inspired)
# Tool blocklist: comma-separated list of blocked tools
# BLOCKED_TOOLS=web_fetch,web_extract,github_clone,replicate_generate_image
# Or use allowlist mode (only listed tools allowed):
# ALLOWED_TOOLS=execute_shell,read_file,write_file,list_directory

# Tool execution timeout in milliseconds (default: 30000 = 30s)
# TOOL_TIMEOUT_MS=30000

# Maximum concurrent tools per user (default: 5)
# MAX_CONCURRENT_TOOLS=5

# Slack (opcional)
SLACK_BOT_TOKEN=xoxb-xxx
SLACK_APP_TOKEN=xapp-xxx
SLACK_SIGNING_SECRET=xxx

# Discord (opcional)
DISCORD_BOT_TOKEN=xxx
DISCORD_ADMIN_USER_IDS=123456789012345678,987654321098765432  # Comma-separated Discord User IDs com permissão de aprovar melhorias
WHATSAPP_QR_CHANNEL_ID=123456789012345678  # Discord Channel ID onde o bot enviará QR codes do WhatsApp

# Telegram (opcional)
TELEGRAM_BOT_TOKEN=xxx

# Multimodal APIs (opcional - para geração de mídia)
REPLICATE_API_TOKEN=r8_xxx
OPENAI_API_KEY=sk-xxx
ELEVENLABS_API_KEY=sk_xxx
